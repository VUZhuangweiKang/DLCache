{
    "title" : "Datasets",    
    "bsonType": "object",
    "required": ["ETag", "ChunkETag", "Key", "Location", "Size", "ChunkSize", "LastModified", "TotalAccessTime", "LastAccessTime"],
    "properties": {
        "ETag": {"bsonType": "string"},
        "ChunkETag": {"bsonType": "string"},
        "Part": {"bsonType": "int"},
        "Key": {"bsonType": "string"},
        "Location": {"bsonType": "string"},
        "Size": {"bsonType": ["long", "int"]},
        "ChunkSize": {"bsonType": ["long", "int"]},
        "Cost": {"bsonType": "double"},
        "Category": {"bsonType": "string"},
        "Status": {
            "bsonType": "object",
            "required": ["code", "active_count", "cool_down_init"],
            "properties": {
                "code": {"bsonType": "int"},
                "active_count": {"bsonType": "int"},
                "cool_down_init": {"bsonType": ["timestamp", "int"]}
            }
        },
        "LastModified": {"bsonType": "timestamp"},
        "InitTime": {"bsonType": "timestamp"},
        "TotalAccessTime": {"bsonType": "int"},
        "LastAccessTime": {"bsonType": "timestamp"},
        "References": {
            "bsonType": "array",
            "items": {
                "bsonType": "timestamp"
            }
        },
        "Jobs": {
            "bsonType": "array",
            "items": {
                "bsonType": "string"
            }
        }
    }
