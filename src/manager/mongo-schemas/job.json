{
    "title" : "Job",
    "bsonType": "object",
    "required": [
        "Meta",
        "QoS",
        "Data"
    ],
    "properties": {
        "Meta": {
            "bsonType": "object",
            "required": [
                "Username",
                "JobId",
                "NodeIP",
                "Datasource",
                "ResourceInfo",
                "CreateTime"
            ],
            "properties": {
                "Username": {"bsonType": "string"},
                "JobId": {"bsonType": "string"},
                "NodeIP": {"bsonType": "string"},
                "Datasource": {
                    "bsonType": "object",
                    "required": [
                        "Name",
                        "Bucket"
                    ],
                    "properties": {
                        "Name": {"bsonType": "string"},
                        "Bucket": {"bsonType": "string"},
                        "Keys": {
                            "bsonType": "array",
                            "items": {"bsonType": "string"}
                        }
                    }
                },
                "ResourceInfo": {
                    "bsonType": "object",
                    "title": "resourceInfo",
                    "properties": {
                        "CPUMemoryFree": {"bsonType": ["long", "int"]},
                        "GPUMemoryFree": {"bsonType": ["long", "int"]}
                    },
                    "description": "GPU and CPU memory resource information"
                },
                "CreateTime": {"bsonType": "timestamp"}
            }
        },
        "QoS": {
            "bsonType": "object",
            "title": "qos",
            "required": [
                "UseCache",
                "LazyLoading"
            ],
            "properties": {
                "UseCache": {"bsonType": "bool"},
                "LazyLoading": {"bsonType": "bool"}
            },
            "description": "QoS configuration"
        },
        "ETags": {
            "bsonType": "array",
            "items": {
                "bsonType": "string"
            }
        }
    }
}